package day7;


public class functions {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
         allDivisors(20);
	}
  static void allDivisors(int num) {
		for(int i=1;i<num;i++) {
			if(num%i==0) {
				System.out.println("even number");
			}else {
				System.out.println("odd number");
			}
		}
	}

}
